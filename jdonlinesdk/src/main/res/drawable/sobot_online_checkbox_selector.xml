<?xml version="1.0" encoding="utf-8"?><!--定义一个带圆角,主题背景矩形-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/sobot_online_status_keep_select_icon" android:state_checked="true" /><!--设置选中图片-->
    <item android:drawable="@drawable/sobot_online_status_keep_select_icon" android:state_selected="true"></item>
    <item android:drawable="@drawable/sobot_online_status_keep_select_icon" android:state_pressed="true"></item>
    <item android:drawable="@drawable/sobot_online_status_keep_normal_icon" android:state_checked="false" /><!--设置未选中图片-->
</selector>