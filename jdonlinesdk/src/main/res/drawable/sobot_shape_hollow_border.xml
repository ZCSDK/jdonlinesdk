<?xml version="1.0" encoding="utf-8"?><!--定义一个带圆角,透明背景,灰色边框的矩形-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--圆角-->
    <corners android:radius="15dp" />
    <!--填充颜色-->
    <solid android:color="@color/sobot_app_transparent" />
    <!--描边-->
    <stroke
        android:width="0.4dp"
        android:color="@color/sobot_online_line_color" />
</shape>