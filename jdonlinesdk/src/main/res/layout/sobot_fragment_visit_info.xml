<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/sobot_online_app_bg_color"
    android:orientation="vertical"
    android:paddingTop="12dp"
    android:paddingRight="15dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_visitor_from"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_search_engines"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_visitor_loading_page"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_loading_page"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_visitor_launch_page"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_launch_page"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_access_channel"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_access_channel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_user_skill"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_skill_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_line_up_time"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_line_up_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_ip_address"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_ip_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_terminal"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_terminal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_system"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_system"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_chat_count"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="@string/online_search_keywords"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_search_keywords"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>


</LinearLayout>